public class Solution {
    public int solve(int[] val, int[] wt, int lt) {
        int dp[][]=new int[wt.length+1][lt+1];
        for(int i=0;i<dp.length;i++)
        {
            dp[i][0]=0;
        }
        for(int i=0;i<dp[0].length;i++)
        {
            dp[0][i]=0;
        }
        for(int i=1;i<dp.length;i++)
        {
            for(int j=1;j<dp[0].length;j++)
            {
                if(j<wt[i-1])
                {
                    dp[i][j]=dp[i-1][j];
                }
                else
                {
                    dp[i][j]=Math.max(val[i-1]+dp[i-1][j-wt[i-1]],dp[i-1][j]);//VERY IMP
                }
            }
        }
        return dp[dp.length-1][dp[0].length-1];
    }
}
